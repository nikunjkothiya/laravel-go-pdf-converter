{
    "name": "nikunjkothiya/laravel-go-pdf-converter",
    "description": "High-performance CSV, Excel and PowerPoint to PDF converter for Laravel, powered by Go for maximum speed and minimal resource usage",
    "keywords": [
        "laravel",
        "pdf",
        "converter",
        "csv",
        "excel",
        "xlsx",
        "powerpoint",
        "pptx",
        "go",
        "golang",
        "high-performance"
    ],
    "homepage": "https://github.com/nikunjkothiya/laravel-go-pdf-converter",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikunj Kothiya",
            "email": "nikunjkothiya@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/process": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NikunjKothiya\\GoPdfConverter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NikunjKothiya\\GoPdfConverter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NikunjKothiya\\GoPdfConverter\\GoPdfServiceProvider"
            ],
            "aliases": {
                "PdfConverter": "NikunjKothiya\\GoPdfConverter\\Facades\\PdfConverter"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "build-go": "cd go-binary && make build-all"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
